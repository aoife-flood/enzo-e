Method {
    gravity {
        solver = "bcg";
    }
 }
 Solver {
     list = [ "bcg", "diagonal" ];
     bcg {
         iter_max = 100;
         monitor_iter = 1;
         type = "bicgstab";
         precondition = "diagonal_D";
     };
     diagonal {
        type = "diagonal";
     }
 }
 Field {
    list += ["diagonal"];
 }

